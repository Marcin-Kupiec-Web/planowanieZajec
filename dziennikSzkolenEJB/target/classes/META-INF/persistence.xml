<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="userManagerPU" transaction-type="JTA">
	    <jta-data-source>java:/datasources/MySqlSzkola</jta-data-source>
	    <class>com.edziennik.planZajec.model.PlanZajecZaplanowane</class>
	    <class>com.edziennik.terminarz.model.Terminarz</class>
	    <class>com.userManager.model.UsersStudent</class>
	     <class>com.userManager.model.User</class>
	    <class>com.edziennik.logi.model.Logi</class>
	    <class>com.edziennik.dniWolne.model.Dniwolne</class>
	    <class>com.edziennik.sale.model.Sale</class>
	    <class>com.plany.model.UserBlokadaPlany</class>
	    <class>com.godzinyZajec.model.Godzinyzajec</class>
	    <class>com.edziennik.logi.model.LogiDaneosobowe</class>
	    	<properties>
	    	    <property name="hibernate.dialect" value="org.hibernate.dialect.MySQL5Dialect" />
	    	    <property name="hibernate.hbm2ddl.auto" value="update" />
	    	    <property name="hibernate.show_sql" value="true" />
	    	    <property name="hibernate.enable_lazy_load_no_trans" value="true"/>
	    	</properties>
	</persistence-unit>
</persistence>
 
